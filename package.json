{
  "name": "paratype",
  "version": "0.3.2",
  "description": "Basic run-time type information for typescript and javascript",
  "main": "./dist/paratype.js",
  "typings": "./dist/paratype.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf temp lib && npm run lint && tsc",
    "build": "rollup -c",
    "postbuild": "npm test && api-extractor run --local && api-documenter markdown --input temp --output docs",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mwikstrom/paratype.git"
  },
  "author": "Mårten Wikström",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mwikstrom/paratype/issues"
  },
  "homepage": "https://github.com/mwikstrom/paratype#readme",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@microsoft/api-documenter": "^7.13.33",
    "@microsoft/api-extractor": "^7.18.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.31.0",
    "jest": "^27.0.6",
    "rimraf": "^3.0.2",
    "rollup": "^2.55.1",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  }
}
